@model ZazasCleaningService.Services.Models.Products.ProductsServiceModel
@{
    ViewData["Title"] = "Details";
}

<!-- banner 2 -->
<div class="banner2-w3ls">

</div>
<!-- //banner 2 -->
<!-- page details -->
<div class="breadcrumb-agile">
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="/Products/Details/@Model.Id">@Model.Id</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page">Order</li>
        </ol>
    </nav>
</div>
<!-- //page details -->
<!-- contact -->
<div class="contact py-5">
    <div class="container py-xl-5 py-lg-3">
        <h2 class="tittle mb-xl-5 mb-4 text-dark text-uppercase font-weight-bold pl-4 py-3">Contact Us</h2>
        <div class="row contact_wthreerow agileits-w3layouts pt-4">
            <div class="col-lg-7 w3l_contact_form">
                <form method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="Id"></label>
                        <input asp-for="Id"
                               type="text"
                               name="ProductId"
                               class="form-control"
                               placeholder="Name"
                               required="">
                        <span asp-validation-for="Id" class="text-danger"></span>
                    </div>
                    <button type="submit" class="btn btn-primary submit">Create Type</button>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- //contact -->